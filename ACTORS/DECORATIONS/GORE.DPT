        HUNT Z 5 A_Custommissile("Blood", -5,0,0)
        HUNT ZZZ 5 
        HUNT ZZZZ 5 
        loop
       Death:
	HUNT Z 0 A_Jump (16,4)
        HUNT Z 0 A_Jump (32,4)
        HUNT Z 0 A_Jump (64,4)
        HUNT Z 0 A_Jump (128,4)
        HUNT Z -1 A_CheckPlayerDone
        Wait
        HUNT Z -1 A_CheckPlayerDone
        Wait
        HUNT Z -1 A_CheckPlayerDone
        Wait
        HUNT Z -1 A_CheckPlayerDone
	}
}

